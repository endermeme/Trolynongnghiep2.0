s:905:"<script type="text/javascript">$("#btnRegister").on("click",function(){$('#btnRegister').html('<i class="fa fa-spinner fa-spin"></i> Đang xử lý...').prop('disabled',true);$.ajax({url:"https://azcloudvps.com/ajaxs/client/register.php",method:"POST",dataType:"JSON",data:{action:"register",username:$("#username").val(),email:$("#email").val(),password:$("#inputNewPassword1").val(),repassword:$("#inputNewPassword2").val(),},success:function(respone){if(respone.status=='success'){cuteToast({type:"success",message:respone.msg,timer:5000});setTimeout("location.href = 'https://azcloudvps.com/';",100);}else{cuteToast({type:"error",message:respone.msg,timer:5000});}1NS3R7N3WL1N3$('#btnRegister').html('Đăng Ký').prop('disabled',false);},error:function(){cuteToast({type:"error",message:'Không thể xử lý',timer:5000});$('#btnRegister').html('Đăng Ký').prop('disabled',false);}});});</script>";