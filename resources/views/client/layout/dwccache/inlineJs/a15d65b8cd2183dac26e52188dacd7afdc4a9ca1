s:445:"<script type="text/javascript">$(function(){var url=window.location.pathname,urlRegExp=new RegExp(url.replace(/\/$/,'')+"$");$('a').each(function(){if(urlRegExp.test(this.href.replace(/\/$/,''))){var href=$(this).parents().eq(0).attr('id');$(this).addClass('active');$('#'+href).addClass('active');Checkhref(href);}});function Checkhref(href){$('a').each(function(){if($(this).attr('href')=="#"+href){$(this).addClass('active');}});}});</script>";