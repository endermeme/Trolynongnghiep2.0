s:478:"<script type="text/javascript">function options(id,element){var checkbox=$('#'+element);if(checkbox.prop("checked")==true){$.ajax({url:'/ajaxs/status.php',type:'POST',dataType:'text',data:{action:"game",id:id,status:checkbox.prop("checked")?'1':'0'}}).done(function(res){});}else if(checkbox.prop("checked")==false){$.ajax({url:'/ajaxs/status.php',type:'POST',dataType:'text',data:{action:"game",id:id,status:checkbox.prop("checked")?'1':'0'}}).done(function(res){});}}</script>";